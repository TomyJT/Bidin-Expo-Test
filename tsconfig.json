{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@app/*": [
        "app/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@store/*": [
        "store/*"
      ]
    }
  },
  "include": [
    "app/**/*",
    "components/**/*",
    "assets/**/*",
    "expo-env.d.ts",
    ".expo/types/**/*.ts"
  ]
}
